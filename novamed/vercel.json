{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.2"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "public/build/$1"
    },
    {
      "src": "/storage/(.*)",
      "dest": "public/storage/$1"
    },
    {
      "src": "/(favicon\\.ico|favicon\\.png|robots\\.txt)",
      "dest": "public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "VERCEL_DEMO_MODE": "true"
  },
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.2",
      "excludeFiles": "{tests/**,node_modules/**}"
    }
  },
  "redirects": [],
  "rewrites": [],
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  }
}

